<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item


        android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:radius="2dp"></corners>
            <stroke android:width="1dp" android:color="@color/bg_call_press"></stroke>
            <solid android:color="@color/bg_call_press"></solid>
        </shape>
    </item>
    <item>

        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:radius="2dp"></corners>
            <stroke android:width="1dp" android:color="@color/bg_call"></stroke>
            <solid android:color="@color/bg_call"></solid>
        </shape>
    </item>
</selector>